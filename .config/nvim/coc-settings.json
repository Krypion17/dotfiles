{
    "languageserver": {
      "haskell": {
        "command": "haskell-language-server-wrapper",
        "args": [
          "--lsp"
        ],
        "rootPatterns": [
          ".stack.yaml",
          ".hie-bios",
          "BUILD.bazel",
          "cabal.config",
          "package.yaml"
        ],
        "filetypes": [
          "hs",
          "lhs",
          "haskell"
        ],
        "initializationOptions": {
          "languageServerHaskell": {
            "hlintOn": true,
            "maxNumberOfProblems": 10,
            "completionSnippetsOn": true
          }
        }
      }
    },
    "suggest.completionItemKindLabels": {
        "method": "  ",
        "function": "  ",
        "variable": "[]",
        "field": "  ",
        "typeParameter": "<>",
        "constant": "  ",
        "class": "  ",
        "interface": "  ",
        "struct": "  ",
        "event": "  ",
        "operator": "  ",
        "module": "  ",
        "property": "  ",
        "enum": "  ",
        "reference": "  ",
        "keyword": "  ",
        "file": "  ",
        "folder": " ﱮ ",
        "color": "  ",
        "unit": "  ",
        "snippet": "  ",
        "text": "  ",
        "constructor": "  ",
        "value": "  ",
        "enumMember": "  "
    },
    "coc.source.around.firstMatch": false,
    "hover.floatConfig": {
	"highlight": "Float",
        "border": true
    },
    "diagnostic.floatConfig": {
	"highlight": "Float",
        "border": true
    },
    "suggest.floatConfig": {
	"highlight": "Float",
        "border": true
    }
}
